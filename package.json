    "private": true,
    "description": "Front-End for the Songbirdz NFT Collection",
    "author": "dry-tortuga",
    "license": "MIT",
    "dependencies": {
        "@coinbase/onchainkit": "^0.34.0",
        "@rainbow-me/rainbowkit": "^2.2.0",
        "@tanstack/react-query": "^5.59.15",
        "@wagmi/core": "^2.13.8",
        "bootstrap": "^5.1.3",
        "bootswatch": "^5.1.3",
        "buffer": "^6.0.3",
        "dayjs": "^1.11.9",
        "ethers": "^6.12.1",
        "react": "^18.2.0",
        "react-bootstrap": "^2.4.0",
        "react-dom": "18.2.0",
        "react-router-dom": "^6.27.0",
        "react-scripts": "^5.0.1",
        "react-select": "^5.7.2",
        "react-virtuoso": "^4.12.3",
        "typescript": "^5.5.4",
        "viem": "^2.21.28",
        "wagmi": "^2.12.19"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@eslint/compat": "^1.0.1",
        "@eslint/js": "^9.3.0",
        "env-cmd": "^10.1.0",
        "eslint": "^8.57.0",
        "globals": "^15.3.0"
    },
    "overrides": {
        "typescript": "^5.5.4"
    },
    "scripts": {
        "copy-abi-backend": "cp ../songbirdz-collection-backend/artifacts/contracts/SongBirdz.sol/SongBirdz.json ./src/abi/SongBirdz.json",
        "start": "npm run copy-abi-backend; react-scripts start",
        "start:staging": "npm run copy-abi-backend;env-cmd -f .env.staging react-scripts start",
        "build": "react-scripts build",
        "build:staging": "env-cmd -f .env.staging npm run build",
        "build:production": "rm -r static-dist/static/; env-cmd -f .env.production npm run build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
